{
  "name": "netflix",
  "version": "1.0.0",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "test": "jest test"
  },
  "dependencies": {
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/push-notification-ios": "^1.2.0",
    "@react-native-firebase/app": "^6.7.1",
    "@react-native-firebase/messaging": "^6.7.1",
    "@react-navigation/bottom-tabs": "^5.3.4",
    "@react-navigation/native": "^5.2.4",
    "@react-navigation/stack": "^5.2.19",
    "appcenter": "3.0.3",
    "appcenter-analytics": "3.0.3",
    "appcenter-crashes": "3.0.3",
    "expo": "^37.0.0",
    "i18n-js": "^3.7.0",
    "jest-expo": "^37.0.0",
    "react": "~16.9.0",
    "react-dom": "16.9.0",
    "react-native": "~0.61.5",
    "react-native-camera": "^3.26.0",
    "react-native-code-push": "^6.2.1",
    "react-native-geocoder": "^0.5.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-notifications": "^3.1.4",
    "react-native-push-notification": "^3.3.1",
    "react-native-reanimated": "^1.7.1",
    "react-native-safe-area-context": "^1.0.0",
    "react-native-screens": "^2.2.0",
    "react-native-testing-library": "^2.1.0",
    "react-native-unimodules": "~0.9.0",
    "react-native-web": "^0.11.7",
    "react-spring": "^8.0.27",
    "styled-components": "^5.1.0",
    "tslib": "^1.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "babel-jest": "~25.2.6",
    "babel-preset-expo": "^8.1.0",
    "jest": "~25.2.6",
    "react-test-renderer": "~16.9.0"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "__tests__/coverage/"
    ],
    "coverageDirectory": "__tests__/coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "private": true
}
